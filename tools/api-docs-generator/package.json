{
  "name": "@cnc-jog-controls/api-docs-generator",
  "version": "1.0.0",
  "description": "Automated API documentation generator for CNC Jog Controls plugins",
  "main": "dist/index.js",
  "bin": {
    "cnc-api-docs": "dist/cli.js"
  },
  "files": [
    "dist/",
    "templates/",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "ts-node src/cli.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "clean": "rimraf dist",
    "prepare": "npm run build"
  },
  "keywords": [
    "api",
    "documentation",
    "generator",
    "typescript",
    "cnc",
    "plugins",
    "markdown"
  ],
  "author": "CNC Jog Controls Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cnc-jog-controls/api-docs-generator.git"
  },
  "dependencies": {
    "commander": "^10.0.0",
    "typescript": "^5.0.0",
    "ts-morph": "^18.0.0",
    "markdown-it": "^13.0.0",
    "highlight.js": "^11.7.0",
    "mustache": "^4.2.0",
    "fs-extra": "^11.1.0",
    "glob": "^8.1.0",
    "chalk": "^5.2.0",
    "ora": "^6.1.0",
    "yaml": "^2.2.1",
    "typedoc": "^0.23.0",
    "jsdoc-to-markdown": "^8.0.0",
    "doctrine": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/markdown-it": "^12.2.0",
    "@types/mustache": "^4.2.0",
    "@types/fs-extra": "^11.0.0",
    "@types/glob": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "rimraf": "^4.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}