{
  "name": "{{pluginName}}",
  "version": "{{version}}",
  "description": "{{description}}",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "src/manifest.json",
    "src/assets/",
    "src/locales/",
    "src/config/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "cnc-plugin dev",
    "build": "cnc-plugin build",
    "build:watch": "cnc-plugin build --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src/**/*.{ts,tsx,json,css}",
    "validate": "cnc-plugin validate",
    "package": "cnc-plugin package",
    "clean": "rimraf dist coverage",
    "prepare": "npm run build",
    "prepack": "npm run build && npm run validate"
  },
  "keywords": [
    "cnc",
    "cnc-jog-controls",
    "plugin",
{{#keywords}}
    "{{.}}",
{{/keywords}}
    "typescript",
    "react"
  ],
  "author": {
    "name": "{{author.name}}",
    "email": "{{author.email}}"{{#author.url}},
    "url": "{{author.url}}"{{/author.url}}
  },
  "license": "{{license}}",
{{#repository}}
  "repository": {
    "type": "git",
    "url": "{{repository}}"
  },
  "bugs": {
    "url": "{{repository}}/issues"
  },
  "homepage": "{{repository}}#readme",
{{/repository}}
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@cnc-jog-controls/plugin-api": "^1.0.0",
    "@cnc-jog-controls/plugin-cli": "^1.0.0",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@testing-library/react": "^13.0.0",
    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/user-event": "^14.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "prettier": "^2.8.0",
    "rimraf": "^4.0.0",
    "typescript": "^4.9.0"
  },
  "peerDependencies": {
    "@cnc-jog-controls/plugin-api": "^1.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}